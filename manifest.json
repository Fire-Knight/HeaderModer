{
    "name": "ModHeader_Chrome",
    "version": "2.0",
    "description": "Modify header of chrome; Download request content",
	"manifest_version": 2,
	"web_accessible_resources": [
		"modHeader.json"
	],
	"permissions":["storage","webRequest","downloads","webRequestBlocking","<all_urls>"],
	"background": {
		"scripts":["background.js"]
	},
	"browser_action":{
		"default_popup": "popup.html"
	}
}